<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>ServletMethods</display-name>
  <welcome-file-list>
    <welcome-file>IndexForGet.html</welcome-file>
    <welcome-file>IndexForPost.html</welcome-file>
  </welcome-file-list>
  <servlet>
  <servlet-name>GetService</servlet-name>
  <servlet-class>get.GetLogin</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>GetService</servlet-name>
  <url-pattern>/login/get</url-pattern>
  </servlet-mapping>
   <servlet>
  <servlet-name>PostService</servlet-name>
  <servlet-class>post.PostLogin</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>PostService</servlet-name>
  <url-pattern>/login/post</url-pattern>
  </servlet-mapping>
</web-app>